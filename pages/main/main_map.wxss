page {
  background-color: #F7F7F7;
  border: 1px solid rgba(0,0,0,0);
}

.page {
    font-size:24rpx;
    font-family:MicrosoftYaHei;
    font-weight:400;
    color:rgba(50,50,50,1);
}

.search-img{
  display: flex;
  align-items: center;
  width:8%;
  margin-left:20px;
}

.map_container{
  position: absolute;
  top: 50px;
  bottom: 0px;
  left: 0;
  right: 0;
}
.map{
  width: 100%;
  height: 100%;
}

.controls {
  position: absolute;
  top:70%;
  width:95%;
  height: 140px;
  margin:10px 10px 0 10px;
  background-color: rgba(255,255,255,1);
  font-family:MicrosoftYaHei;
  font-weight:400;
  color:rgba(50,50,50,1);
}

.tip-txt{
  width:100%;
  font-size:18px;
  text-align: center;
}

.link-img {
  position: absolute;
  left:90%;
  top:12px;
}

.title{
  margin:10px 0;
}

.detail_flex{
  display:flex;
  align-items:baseline;
}

.detail{
  display:flex;
  align-items:baseline;
  margin:15px;
  font-size:12px;
  color:rgba(100,100,100,1);
}


.d-col{
  font-size:18px;
  color:#7F94FF;
  font-family: MicrosoftYaHei-Bold;
}

.d-col2{
  margin-right:53px;
  
}
.red{
  color:#FF3434;
}

.d-col3{
  width:55%;
}

.d-col-img{
  margin-right:10px;
}
